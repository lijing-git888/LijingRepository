        {
          "Host": "localhost",
          "Port": 5226
        }
      ],
      "UpstreamPathTemplate": "/BasicDataApi/swagger.json",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/Demo/{everything}",
      "DownstreamScheme": "http",
